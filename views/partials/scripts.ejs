<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<script type="text/javascript" src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
<script>
    $(function () {

        var deployButton = $('#deploy-btn');
        var fromInput = $('#from-branch-input');
        var toInput = $('#to-branch-input');
        var logsBox = $('#deploy-logs');

        function enableOrDisableDeployButton(disable) {
            if (disable) {
                deployButton.attr('disabled', 'disabled');
            } else {
                deployButton.removeAttr('disabled');
            }
        }
        var logs = [];
        deployButton
            .click(function () {
                var data = {};
                data.fromBranch = fromInput.val();
                data.toBranch = toInput.val();
                $.ajax({
                    type: 'POST',
                    data: JSON.stringify(data),
                    contentType: 'application/json',
                    url: '/deployer',
                    success: function (data) {
                        console.log('success');
                        console.log(JSON.stringify(data));
                    }
                });

            })
    })
</script>